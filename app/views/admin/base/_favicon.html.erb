<%#
# Favicon Partial

This partial imports the necessary favicon on each page.
%>

<%= favicon_link_tag image_path('apple-touch-icon.png'), rel: 'apple-touch-icon', sizes: '180x180' %>
<%= favicon_link_tag image_path('favicon.ico') %>
<%= favicon_link_tag image_path('favicon-32x32.png'), sizes: '32x32' %>
<%= favicon_link_tag image_path('favicon-16x16.png'), sizes: '16x16' %>
<%= favicon_link_tag image_path('brand_logo.svg'), type: 'image/svg+xml' %>

<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">
